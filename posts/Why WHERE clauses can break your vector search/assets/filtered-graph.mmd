%%{init: {'theme': 'default', 'themeVariables': { 'fontSize': '14px' }}}%%
flowchart LR
    subgraph filtered["Filtered set (Apple + Google)"]
        direction LR

        subgraph apple["Apple"]
            direction TB
            MA["A"] ===|brand| MB["B"]
            MA ===|brand| ME["E"]
            MB ===|brand| ME
        end

        subgraph google["Google"]
            direction TB
            MC["C"] ===|brand| MD["D"]
            MC ===|brand| MG["G"]
            MD ===|brand| MG
        end

        MA ---|similarity| MC
        MB ---|similarity| MG
    end

    subgraph removed["Removed (Sony)"]
        direction TB
        MF["F"]
        MH["H"]
        MI["I"]
    end

    style filtered fill:#e8f5e9,stroke:#2e7d32
    style apple fill:#e8f5e9,stroke:#2e7d32
    style google fill:#e3f2fd,stroke:#1565c0

    style removed fill:#fafafa,stroke:#bdbdbd,stroke-dasharray: 5 5
    style MF fill:#f5f5f5,stroke:#bdbdbd,color:#9e9e9e
    style MH fill:#f5f5f5,stroke:#bdbdbd,color:#9e9e9e
    style MI fill:#f5f5f5,stroke:#bdbdbd,color:#9e9e9e
