digraph MergedGraph {
    graph [
        bgcolor=white
        fontname="Helvetica"
        fontsize=14
        pad=0.5
        nodesep=0.6
        ranksep=0.8
        dpi=150
        label="Merged graph: similarity edges + brand subgraph edges"
        labelloc=t
        fontcolor="#333333"
    ]
    
    node [
        fontname="Helvetica"
        fontsize=11
        style="filled"
        shape=circle
        width=0.6
        fixedsize=true
    ]
    
    // Apple nodes (green)
    A [label="A" fillcolor="#d4edda" color="#28a745" penwidth=2]
    B [label="B" fillcolor="#d4edda" color="#28a745" penwidth=2]
    E [label="E" fillcolor="#d4edda" color="#28a745" penwidth=2]
    
    // Sony nodes (purple) - bridge nodes
    F [label="F" fillcolor="#e1bee7" color="#7b1fa2" penwidth=2]
    H [label="H" fillcolor="#e1bee7" color="#7b1fa2" penwidth=2]
    I [label="I" fillcolor="#e1bee7" color="#7b1fa2" penwidth=2]
    
    // Google nodes (blue)
    C [label="C" fillcolor="#cce5ff" color="#1565c0" penwidth=2]
    D [label="D" fillcolor="#cce5ff" color="#1565c0" penwidth=2]
    G [label="G" fillcolor="#cce5ff" color="#1565c0" penwidth=2]
    
    // Layout: 3 rows, interleaved
    {rank=same; A; F; C}
    {rank=same; B; H; D}
    {rank=same; E; I; G}
    
    // Brand subgraph edges (thick, colored)
    // Apple brand edges
    A -> B [dir=none color="#28a745" penwidth=3 style=bold]
    A -> E [dir=none color="#28a745" penwidth=3 style=bold]
    B -> E [dir=none color="#28a745" penwidth=3 style=bold]
    
    // Sony brand edges
    F -> H [dir=none color="#7b1fa2" penwidth=3 style=bold]
    F -> I [dir=none color="#7b1fa2" penwidth=3 style=bold]
    H -> I [dir=none color="#7b1fa2" penwidth=3 style=bold]
    
    // Google brand edges
    C -> D [dir=none color="#1565c0" penwidth=3 style=bold]
    C -> G [dir=none color="#1565c0" penwidth=3 style=bold]
    D -> G [dir=none color="#1565c0" penwidth=3 style=bold]
    
    // Similarity edges (thin, gray, dashed) - cross-brand
    A -> F [dir=none color="#999999" penwidth=1.5 style=dashed label="sim" fontsize=9 fontcolor="#999999"]
    F -> C [dir=none color="#999999" penwidth=1.5 style=dashed]
    B -> H [dir=none color="#999999" penwidth=1.5 style=dashed]
    H -> D [dir=none color="#999999" penwidth=1.5 style=dashed]
    E -> I [dir=none color="#999999" penwidth=1.5 style=dashed]
    I -> G [dir=none color="#999999" penwidth=1.5 style=dashed]
    
    // Direct Apple-Google similarity edges
    A -> C [dir=none color="#999999" penwidth=1.5 style=dashed]
    B -> G [dir=none color="#999999" penwidth=1.5 style=dashed]
    
    // Legend
    subgraph cluster_legend {
        label="Legend"
        fontsize=10
        style="rounded"
        fillcolor="#fafafa"
        color="#cccccc"
        
        leg_brand [label="brand\nedge" shape=box fillcolor=white fontsize=9]
        leg_sim [label="similarity\nedge" shape=box fillcolor=white fontsize=9]
        
        leg_brand -> leg_sim [dir=none penwidth=3 color="#666666" style=bold label=" thick"]
        leg_sim -> leg_brand [dir=none penwidth=1.5 color="#999999" style=dashed label=" dashed" constraint=false]
    }
}
